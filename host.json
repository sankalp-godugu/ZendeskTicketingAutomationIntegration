{
  "version": "2.0",
  "logging": {
    "applicationInsights": {
      "samplingExcludedTypes": "Request",
      "samplingSettings": {
        "isEnabled": true
      }
    },
    "logLevel": {
      "Function.FunctionName": "Information",
      "Default": "None"
    },
    "azureMonitorTrace": true,
    "console": {
      "isEnabled": false
    },
    "file": {
      "isEnabled": false
    },
    "applicationInsightsSamplingPerMessage": 1
  }
}
